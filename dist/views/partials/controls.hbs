<form action="../user/procesar" method="post">
  <table border="1">
    <theader> {{!-- Encabezado - th: para negrita --}}
      <th>Seleccion</th>
      <th>ID</th>
      <th>Nombre</th>
      <th>Password</th>
      <th>Rol</th>
      <th>Editar</th>
      <th>Eliminar</th>
    </theader>
        {{#each users}}
        <tr> 
          <td>
            <input type="checkbox" name="usuario" value="{{nombre}}">
          </td> 
          <td>{{Id}}</td> 
          <td>{{Nombre}}</td> 
          <td>{{Password}}</td>
          <td>{{Perfil}}</td>
          <td align="center"><a class="btn btn-primary" href="/user/update/{{Id}}">Editar</a></td>
          <td><a class="btn btn-danger" href="/user/delete/{{Id}}">Eliminar</a></td>   
        </tr>
        {{else}}
        <tr>
          <td colspan="4">No hay mas filas</td>
        </tr>
        {{/each}}
  </table>
  <button class="btn btn-success">Enviar</button>
</form>