<body style="background-color: #c9fcdb;">
    <div class="contenedor">        
        <div style="width:83%;margin-left:300px;">
            <BR>
            <h4>Listado de Proveedores</h4>
            <button class="btn btn-primary" style="clear:both;float:right;" onclick="window.location.href='add';">Agregar</button>
            <br>
        </div>
        <br>
        <table class="table table-striped" style="width:83%;;margin-left:300px;">
            <theader>
                <th>Tipo Documento</th>
                <th>Numero Documento</th>
                <th>Razon Social</th>
                <th>Email</th>
                <th>Direccion</th>
                <th>Localidad</th>
                <th>Provincia</th>
                <th>Codigo Postal</th>
                <th>Descripcion</th>
                <th>Telefono</th>
                <th>Accion</th>
            </theader>
            {{#each supplier}}
            <tr>
                <td>{{TipoDocumento}}</td>
                <td>{{NumeroDocumento}}</td>
                <td>{{RazonSocial}}</td>
                <td>{{Email}}</td>
                <td>{{Direccion}}</td>
                <td>{{Localidad}}</td>
                <td>{{Provincia}}</td>
                <td>{{CodigoPostal}}</td>
                <td>{{Descripcion}}</td>
                <td>{{Telefono}}</td>
                <td><a class="btn btn-info" href="/supplier/update/{{Id}}">Editar</a> <a class="btn btn-danger" href="/supplier/delete/{{Id}}">Eliminar</a></td>
            </tr>
            {{else}}
            <tr>
                <td colspan="11">No hay mas filas</td>
            </tr>
            {{/each}}
        </table>
         <div style="width:80%;;margin-left:320px;">        
            <div align="center">
                <br>            
                <button class="btn btn-success" onclick="window.location.href='/user/home';" align="center">Volver</button>
            </div>
        </div>
    </div>
</body>