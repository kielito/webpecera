<body style="background-color: #d1ffe2;">
    <div class="contenedor">        
    <div style="width:70%;margin-left:300px;">
        <BR>
        <h4>Listado de Clientes</h4>
    </div>
    <BR>
    <table class="table table-striped" style="width:83%;;margin-left:300px;">
        <theader>
            <th>Id</th>
            <th>TipoDocumento</th>
            <th>NumeroDocumento</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Email</th>
            <th>Direccion</th>
            <th>Localidad</th>
            <th>Provincia</th>
            <th>CodigoPostal</th>
            <th>Accion</th>
        </theader>
        {{#each clients}}
        <tr>
            <td>{{Id}}</td>
            <td>{{TipoDocumento}}</td>
            <td>{{NumeroDocumento}}</td>
            <td>{{Nombre}}</td>
            <td>{{Apellido}}</td>
            <td>{{Email}}</td>
            <td>{{Direccion}}</td>
            <td>{{Localidad}}</td>
            <td>{{Provincia}}</td>
            <td>{{CodigoPostal}}</td>       
            <td><a class="btn btn-info" href="/cliente/update/{{id}}">Editar</a> <a class="btn btn-danger" href="/cliente/delete/{{id}}">Eliminar</a></td>
        </tr>
        {{else}}
        <tr>
            <td colspan="10">No hay mas filas</td>
        </tr>
        {{/each}}
    </table>
    <div style="width:80%;;margin-left:320px;">        
        <div align="center">
            <BR>
            <button class="btn btn-primary" onclick="window.location.href='#';" align="center">Agregar</button>
            <button class="btn btn-success" onclick="window.location.href='http://localhost:3000/user/home';" align="center">Volver</button>
        </div>
    </div>
</div>
</body>